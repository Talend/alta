import java.io.*;

File file = new File(basedir, "target/generated-test-resources/alta/axis-version");
if (!file.exists()) {
    throw new IllegalStateException(file + " doesn't exist");
}
BufferedReader in = new BufferedReader(new InputStreamReader(new FileInputStream(file), "utf-8"));
String content = in.readLine();
if (!"1.3".equals(content) || in.readLine() != null) {
    throw new IllegalStateException("Unexpected content");
}
in.close();
